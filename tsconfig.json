{
  "extends": "@adonisjs/tsconfig/tsconfig.app.json",
  "compilerOptions": {
    "rootDir": "./",
    "outDir": "./build",
    "jsx": "react-jsx",
    "paths": {
      "#instagram/*": ["./app/auth/instagram/*"],
      "#user/*": ["./app/user/*"],
      "#auth/*": ["./app/auth/*.ts"],
      "#utils/*": ["./app/utils/*.ts"],
      "#exceptions/*": ["./app/exceptions/*.ts"]
    }
  },
  "exclude": ["./inertia/**/*", "node_modules", "build"]
}
